{
  "name": "@serverless-stack/resources",
  "version": "1.0.12",
  "homepage": "https://serverless-stack.com",
  "description": "Helper AWS CDK constructs for Serverless Stack apps.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "cdk-version-check": "node ./bin/aws-cdk-version-check.js",
    "build": "tsc --rootDir src --outDir dist && yarn build:stub && yarn build:rds-migrator && yarn build:bridge",
    "build:stub": "cd assets/stub && yarn && cd ../.. && node ./bin/create-stub-zip.js",
    "build:rds-migrator": "cd assets/RDS/migrator && yarn && cd ../../.. && node ./bin/copy-rds-migrator.js",
    "build:bridge": "node ./bin/build-bridge-client.js",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "author": {
    "name": "Serverless Stack",
    "url": "https://serverless-stack.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serverless-stack/serverless-stack.git",
    "directory": "packages/resources"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.22.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "2.22.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.22.0-alpha.0",
    "@aws-cdk/aws-appsync-alpha": "2.22.0-alpha.0",
    "@graphql-tools/load-files": "^6.3.2",
    "@graphql-tools/merge": "^6.2.14",
    "@serverless-stack/core": "1.0.12",
    "archiver": "^5.3.0",
    "aws-cdk-lib": "2.22.0",
    "chalk": "^4.1.0",
    "constructs": "^10.0.29",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.7",
    "graphql": "^16.5.0",
    "zip-local": "^0.3.4",
    "zod": "^3.14.3"
  },
  "devDependencies": {
    "@sls-next/lambda-at-edge": "^3.7.0-alpha.7",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.6",
    "@types/glob": "^7.2.0",
    "@types/node": "^14.0.27",
    "esbuild-jest": "^0.5.0",
    "typedoc": "^0.22.13"
  },
  "gitHead": "8ac2d0abc11d5de721c87658bb445e3d6c211dcf"
}
